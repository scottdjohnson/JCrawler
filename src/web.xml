<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/j2ee/dtds/web-app_2_3.dtd">

<web-app>
<servlet>
    <servlet-name>JCrawlerServlet</servlet-name>
    <servlet-class>scottdjohnson.jcrawler.JCrawlerServlet</servlet-class>
</servlet>

<servlet-mapping>
     <!-- For any URL starting with /content/, the rewriter servlet will be called -->
     <servlet-name>JCrawlerServlet</servlet-name>
     <url-pattern>/JCrawlerServlet</url-pattern>
</servlet-mapping>
</web-app>

