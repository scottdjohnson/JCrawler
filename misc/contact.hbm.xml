<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="scottdjohnson.binarytree.URLNode" table="url_list">
		<id name="Key" type="long" column="url_key" >
			<generator class="increment"/>
		</id>

	  <property name="ParentKey" column="parent_key" type="long" />

	  <property name="Url" column="url" type="string" />

   </class>

</hibernate-mapping>