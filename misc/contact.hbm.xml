<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.scottdjohnson.node.URLNode" table="url_list">
		<id name="Key" type="long" column="url_key" >
			<generator class="increment"/>
		</id>

	<property name="ParentKey" column="parent_key" type="long" />
	<property name="Url" column="url" type="string" />
	<property name="Name" column="link_name" type="string" />
	<property name="TimeCrawled" column="time_crawled" type="timestamp" />
	

   </class>

</hibernate-mapping>
